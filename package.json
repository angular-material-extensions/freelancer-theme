{
  "name": "@angular-material-extension/freelancer-theme",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "build:doc": "cd ../ && gulp build:doc",
    "build:prod": "ng build --prod ",
    "build:ci": "ng build --prod --build-optimizer --base-href /freelancer-theme/ --deploy-url /freelancer-theme/",
    "build:client-and-server-bundles": "npm run env -s && ng build --prod && ng run freelancer-theme:server:production",
    "build:client-and-server-bundles:ci": "npm run env -s && npm run build:ci && ng run freelancer-theme:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:prerender:ci": "npm run build:client-and-server-bundles:ci && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "clean": "rm -rf dist",
    "codecov": "codecov",
    "compile:server": "tsc -p server.tsconfig.json",
    "deploy:demo": "ngh --dir dist/browser/ --message=\"chore(demo): deploy new version\"",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate:prerender": "cd dist && node prerender",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,scss}\"",
    "postinstall": "npm run prettier -s",
    "precommit": "pretty-quick --staged",
    "generate": "ng generate",
    "semantic-release": "semantic-release",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js --port 4210",
    "serve:prod": "ng serve --prod",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.8",
    "@angular/compiler": "^7.2.8",
    "@angular/core": "^7.2.8",
    "@angular/flex-layout": "7.0.0-beta.24",
    "@angular/forms": "^7.2.8",
    "@angular/http": "^7.2.8",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "^7.2.8",
    "@angular/platform-browser-dynamic": "^7.2.8",
    "@angular/platform-server": "^7.2.8",
    "@angular/router": "^7.2.8",
    "@angular/service-worker": "^7.2.8",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-translate/core": "^11.0.1",
    "angulartics2": "^7.4.1",
    "core-js": "^2.6.9",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.14",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^5.0.1",
    "ngx-webstorage": "^3.0.2",
    "rxjs": "^6.4.0",
    "ts-loader": "^5.3.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.5",
    "@angular/cli": "^7.3.5",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.2.8",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.4",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.2.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/release-notes-generator": "^7.2.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.136",
    "@types/node": "^10.14.12",
    "angular-cli-ghpages": "^0.5.3",
    "codecov": "^3.2.0",
    "codelyzer": "^4.4.4",
    "express": "^4.17.1",
    "hads": "^1.7.2",
    "htmlhint": "^0.10.3",
    "http-server": "^0.11.1",
    "https-proxy-agent": "^2.2.2",
    "husky": "^1.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "puppeteer": "^1.18.1",
    "reflect-metadata": "^0.1.13",
    "semantic-release": "^15.13.18",
    "stylelint": "~9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "^3.9.1",
    "ts-node": "^8.3.0",
    "tslint": "~5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.4",
    "webpack-cli": "^3.3.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/angular-material-extensions/freelancer-theme.git"
  }
}
